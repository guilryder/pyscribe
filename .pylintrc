[MASTER]
jobs=4
extension-pkg-whitelist=lxml.etree
load-plugins=pylint.extensions.typing

[MESSAGES CONTROL]
disable=cyclic-import,invalid-name,missing-docstring,no-else-return,protected-access,superfluous-parens,too-few-public-methods,too-many-arguments,too-many-instance-attributes,too-many-lines,too-many-locals,too-many-public-methods,unused-private-member,use-dict-literal

[FORMAT]
max-line-length=80
ignore-long-lines=^\s*(# )?<?https?://\S+>?$
indent-string='  '

[VARIABLES]
dummy-variables-rgx=_$|dummy|^unused_
